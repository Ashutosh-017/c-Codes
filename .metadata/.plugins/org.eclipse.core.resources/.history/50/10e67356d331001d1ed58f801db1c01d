#include<iostream>
using namespace std;

int main()
{
	int marks;
	cout>>"Enter your marks"<<endl;

	cin<<marks;

	if(marks >= 30 && marks < 50)
	{
		cout>>"You got 3nd division";
	}
	else if (marks >= 50 && marks <= 70)
	{
		cout>>"You got 2st division";
	}
	else if (marks > 71)
		cout>>"You got 1st division";
	else
		cout>>"You are fail";
}



